{
	"name": "@kitschpatrol/log",
	"version": "0.0.0",
	"description": "A library project.",
	"keywords": [
		"npm-package",
		"logging",
		"loglayer",
		"log"
	],
	"homepage": "https://github.com/kitschpatrol/log",
	"bugs": "https://github.com/kitschpatrol/log/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/log.git"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/lib/index.js",
			"types": "./dist/lib/index.d.ts"
		}
	},
	"main": "./dist/lib/index.js",
	"module": "./dist/lib/index.js",
	"files": [
		"dist/*"
	],
	"scripts": {
		"build": "tsdown --minify --platform neutral --tsconfig tsconfig.build.json",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && pnpm publish --otp $(op read &#x27;op://Personal/Npmjs/one-time password?attribute&#x3D;otp&#x27;)",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@types/node": "^20.19.21"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.7.4",
		"@loglayer/context-manager": "^1.1.6",
		"@loglayer/transport-log-file-rotation": "^2.1.7",
		"bumpp": "^10.3.1",
		"filenamify": "^7.0.0",
		"loglayer": "^6.9.1",
		"read-package-up": "^11.0.0",
		"serialize-error": "^12.0.0",
		"tsdown": "^0.15.7",
		"typescript": "~5.9.3"
	},
	"packageManager": "pnpm@10.18.3",
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
